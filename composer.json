{
  "name": "allandereal/pesapal-laravel",
  "type": "project",
  "description": "PesaPal payment driver for Laravel.",
  "keywords": ["payment", "laravel", "ecommerce", "e-commerce", "headless", "allandereal", "mobile money", "cart", "pesapal"],
  "license": "MIT",
  "authors": [
    {
      "name": "Allan Dereal",
      "homepage": "https://github.com/allandereal"
    }
  ],
  "require": {
      "php": "^8.2",
      "laravel/framework": "^10.0|^11.0"
  },
  "autoload": {
      "psr-4": {
          "AllanDereal\\PesaPal\\": "src/"
      }
  },
  "extra": {
    "pesapal": {
        "name": "Pesapal Payments"
    },
    "laravel": {
        "providers": [
            "AllanDereal\\PesaPal\\PesaPalServiceProvider"
        ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
